{
  "manifest_version": 2,

  "name": "Aweb",
  "description": "Front web framework search engine",
  "version": "1.0",

  "default_locale": "en",

  "permissions": [
    "tabs",
    "notifications",
    "idle",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "html/popup.html"
  }
}